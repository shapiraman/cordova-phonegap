  top:0px;
  bottom: 65px;
}
.b_meal_plan h1{
  margin: 0;
  text-align: center;
  font-size: 27px;
  letter-spacing: -0.41px;
  padding: 10px 34px; 
}
._daily_calories ._calories{
  color: #d20086;
}
.b_meal_plan ._daily_calories{
  background: #d8d8d8;
  color: #4a4a4a;
  text-align: center;
  padding: 10px 34px;
  font-size: 12px;
  color: #4a4a4a;
  letter-spacing: -0.18px;
}
.b_meal_plan ._all_calories{
  padding: 0 0px 35px;
}
.b_meal_plan ._all_calories ._item{
  font-size: 0;
}
._all_calories ._item{
  padding: 22px 0 8px;
  border-bottom: 1px solid #979797;
  
}
._all_calories ._item > div{
  display: inline-block;
  vertical-align: middle;
  font-size: 12px;
}
.b_meal_plan ._all_calories ._day{
  font-size: 27px;
  letter-spacing: -0.41px;
  color: #4a4a4a;
  width: 35%;
  margin-right:2%
}
.b_meal_plan ._all_calories ._add_dish{
/*  width: 55%;  meal plan*/ 
}

.b_meal_plan ._all_calories ._button{
  
}
.b_meal_plan ._btn_add{
  margin-right: 10px;
  display: inline-block;
  vertical-align: middle;
  width: 31px;
  height: 31px;
  border-radius: 50%;
  border: 1px solid #979797;
  background: none;
  padding: 0;
  position: relative;
}
.b_meal_plan ._btn_add:before{
  position: absolute;
  content: '';
  display: block;
  width: 1px;
  height: 70%;
  top: 50%;
  left: 50%;
  transform: translate(0,-50%);
  background: #979797;
}
.b_meal_plan ._btn_add:after{
  position: absolute;
  content: '';
  display: block;
  width: 70%;
  height: 1px;
  top: 50%;
  left: 50%;
  transform: translate(-50%,0);
  background: #979797;
  margin-top: -1px;
}


/* ==================== */
._all_day .item{
  border: none;
  padding: 10px 0 0;
}
._all_day .item + .item{
  margin: 14px 0 0;
  border-top: 1px solid #979797;
}
._all_calories ._item ._all_day{
  width: 100%;
}
._delete_recepie{
  background: #fd0000;
  color: #fff;
  font-size: 17px;
  letter-spacing: -0.41px;
  border: none;
  line-height: 33px;
  transition: all .2s linear;
  width: 50px;
  text-align: center;
}
._all_day .item-options{
  height: calc(100% - 10px);
  bottom: 0;
  top: auto;
}
._all_day .item-content{
  padding: 0;
}
.item-content ._img{
  width: 30%;
  max-width: 90px;
  display: inline-block;
  vertical-align: top;
  float: right;
  position: relative;
}
.item-content ._img img{
  max-width: 100%;
  vertical-align: top;
}
._item ._recepie_description{
  width: 70%;
  display: inline-block;
  vertical-align: top;
  float: right;
  padding: 0 25px 0 0;
}
._recepie_description h3{
  font-size: 17px;
  letter-spacing: -0.41px;
  margin: 0 0 5px;
}
._recepie_description ._calories{
  font-size: 17px;
  letter-spacing: -0.41px;
  font-weight: 700;
  color: #c81587;
}
/* ==================== */

._all_calories ._item ._recepie{
  width: 100%;
  overflow: hidden;
  display: block;
}
._all_calories ._item ._recepie + ._recepie{
  margin: 14px 0 0;
  padding: 10px 0 0;
  border-top: 1px solid #979797;
}
._item ._recepie ._img{
  width: 30%;
  max-width: 90px;
  display: inline-block;
  vertical-align: top;
  float: right;
  position: relative;
}
._item ._recepie ._img img{
  max-width: 100%;
  vertical-align: top;
}

.all_dish{
  overflow: hidden;
  margin-bottom: 140px
}
.all_dish ._recipies_item{
  width: 50%;
  padding: 0 2px;
  text-align: center;
  text-decoration: none;
  display: block;
  float: right;
}
.all_dish ._img{
  width: 100%;
  padding: 0 0 60.25%;
  overflow: hidden;
  position: relative;
  margin: 0 0 8px;
}
.all_dish ._img img{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  max-width: 105%;
}
.all_dish ._title{
  letter-spacing: -0.41px;
  color: #4a4a4a;
  font-size: 17px;
  white-space: nowrap;
}
