/* animations */

.scrolled {
  transition: transform $duration ease;
  transform: translate3d(0, 0, 0);
  &.ng-enter {
    transform: translate3d(100%, 0, 0);  // Move to the right (up to 100% of width)
  }
  &.ng-enter-active {
    transform: translate3d(0, 0, 0);
  }
  &.ng-leave {
    transform: translate3d(0, 0, 0);
  }
  &.ng-leave-active {
    transform: translate3d(-100%, 0, 0);  
  }
}

.backward {
  .scrolled {
    &.ng-enter {
        transform: translate3d(-100%, 0, 0);
    }
    &.ng-enter-active {
        transform: translate3d(0, 0, 0);
    }
    &.ng-leave {
        transform: translate3d(0, 0, 0);
    }
    &.ng-leave-active {
        transform: translate3d(100%, 0, 0);
    }
  }
}

.iframe-container {
  height:100%;
} 

/* photos */

/* The Modal (background) */

.modal { /* Hidden by default */
    position: fixed; /* Stay in place */
    z-index: 1; /* Sit on top */
    padding-top: 100px; /* Location of the box */
    left: 0;
    top: 0;
    width: 100%%; /* Full width */
    height: 7%%; /* Full height */
    overflow: auto; /* Enable scroll if needed */
    background-color: rgb(0,0,0); /* Fallback color */
    background-color: rgba(0,0,0,0.9); /* Black w/ opacity */
}

/* Modal Content (Image) */
.modal-content {
    margin: auto;
    display: block;
    width: 80%;
    max-width: 700px;
}

.photos-modal {
  background-color: rgba(0,0,0,0.8);
  width: 100%;
  height: 100%;
  overflow-y: scroll !important;
  position: absolute;
  top: 0;
  right: 0;
  display: none;
}
.photos-modal-container {
  width: 100%;
  height: 100%;
  position: relative;
}
.photos-modal-content {
 
  top:50%;
  left:50%;
  transform:translate:(-50%)
}

/* Caption of Modal Image (Image Text) - Same Width as the Image */
#caption {
    margin: auto;
    display: block;
    width: 80%;
    max-width: 700px;
    text-align: center;
    color: #ccc;
    padding: 10px 0;
    height: 150px;
}

/* Add Animation - Zoom in the Modal */
.modal-content, #caption { 
    animation-name: zoom;
    animation-duration: 0.6s;
}

@keyframes zoom {
    from {transform:scale(0)} 
    to {transform:scale(1)}
}

/* The Close Button */
.close {
    position: absolute;
    top: 15px;
    right: 35px;
    color: #f1f1f1;
    font-size: 40px;
    font-weight: bold;
    transition: 0.3s;
}

.close:hover,
.close:focus {
    color: #bbb;
    text-decoration: none;
    cursor: pointer;
}

/* video */


.videogular-container {
  width: 45%;
  height: 40px;
  overflow: hidden;
  float:right;
  position: relative;
  bottom: 7px;
  height: 60px
}
