	position: absolute;
	top: 0;
	right: 0;
	height: 70px;
	width: 100%;
	color: rgb(255,255,255);
	text-align: center;
	line-height: 70px;
	font-size: 26px;
}
.boxes-pieces {
	width: 100%;
	height: 100%;
}
.boxes-piece {
	display: table;
	border-collapse: collapse;
	width: 50%;
	height: 25%;
	box-sizing: border-box;
	border: 1px solid rgb(175,63,130);
	background-color: rgba(0,0,0,.47);
	color: #ffffff;
	font-size: 18px;
	text-align: center;
}
.box1 {
	border-left: 0;
}
.box0 {
	border-right: 0;
}
.boxes-piece span {
    display: table-cell;
    vertical-align: middle;
    word-break: break-word;
	word-wrap: break-word;
    padding: 0 10px;
}

