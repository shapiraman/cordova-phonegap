@import url("../fonts/fonts.css");

body {
	font-family: 'Alef-Regular' !important;
}
.h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6 {
	color:white!important;
} 
.ql-editor {
	text-align: right !important;
}

.englishText {
